	background-color: #bfbbbb;
}
.productMark span {
	display: block;
	font-size: 0.5rem;
	color: #fff;
}
.productMark span:after {
	position: absolute;
    top: 19px;
    left: 0px;
    width: 0;
    height: 0;
    border: 3px solid #999;
    border-top-color: transparent;
    border-left-color: transparent;
    border-right-color: transparent;
    display: block;
    content: ' ';
    -moz-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    border-bottom-color: #bfbbbb !important
}
.productMark-wrap {
	position: absolute;
    top: -3px;
    right: 0;
    z-index: 1;
    width: 80px;
    height: 80px;
